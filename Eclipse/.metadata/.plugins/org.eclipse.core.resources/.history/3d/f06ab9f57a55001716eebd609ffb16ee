/** A class that implements a node in a graph with position (vertex) and distance to destination for better short path finding. **/
package roadgraph;

class Ride_Distance{
    
    protected Ride_Distance(int position, double distance) {
        this.position = position;
        this.distance = distance;
    }

    protected  int position;
    protected double distance;
    
    @Override
    public String toString()
    {
     return position+";"+distance;   
    }
}